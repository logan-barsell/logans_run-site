{
  "name": "bandsyte-platform",
  "version": "1.0.0",
  "description": "Professional band website platform by Bandsyte",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "generate-secret": "node scripts/generateJWTSecret.js",
    "migrate:dev": "node scripts/run-prisma-owner.js migrate dev --schema db/prisma/schema.prisma",
    "migrate:gen": "node -e \"const name = process.argv[1] || 'migration_' + Date.now(); require('child_process').execSync('node scripts/run-prisma-owner.js migrate dev --name ' + name + ' --schema db/prisma/schema.prisma', {stdio: 'inherit'})\"",
    "migrate:deploy": "node scripts/run-prisma-owner.js migrate deploy --schema db/prisma/schema.prisma",
    "migrate:status": "node scripts/run-prisma-owner.js migrate status --schema db/prisma/schema.prisma",
    "seed:dev": "node scripts/seed-core-dev.js",
    "seed:full": "node scripts/seed-full-dev.js",
    "db:export": "pg_dump $DATABASE_URL > backup-$(date +%Y%m%d-%H%M%S).sql",
    "db:import": "psql $DATABASE_URL < $1",
    "db:import-docker": "docker exec -i bandsyte-postgres psql -U bandsyte_user -d bandsyte_dev < $1",
    "db:reset": "npm run migrate:deploy && npm run seed:full"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.873.0",
    "@prisma/client": "^6.15.0",
    "@stripe/react-stripe-js": "^1.10.0",
    "@stripe/stripe-js": "^1.35.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-rate-limit": "^8.0.1",
    "express-status-monitor": "^1.3.4",
    "helmet": "^8.1.0",
    "html-minifier-terser": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.19",
    "redis": "^5.8.2",
    "stripe": "^10.2.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "prisma": "^6.15.0"
  }
}
